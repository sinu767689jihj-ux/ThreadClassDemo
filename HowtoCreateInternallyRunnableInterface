//This is used for creating/implementing Runnable interface
public class HowtoCreateInternallyRunnableInterface implements Runnable{
    public void run()
    {
        for(int i=0;i<10;i++)
        {
            System.out.println("Thread Executing:"+i);
        }
    }
    public static void main(String[] args)
    {
        HowtoCreateInternallyRunnableInterface t1=new HowtoCreateInternallyRunnableInterface();
        Thread t=new Thread(t1);
        t.start();
        Thread t2=new Thread(new HowtoCreateInternallyRunnableInterface());
        t2.start();
    }
}
